<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.koralzakai.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/quiz_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/quiz_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartingScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuizActivity" />
        <activity android:name=".difficulty_adapter_activity"></activity>
        <service
            android:name=".MyService"
            android:enabled="true" />
    </application>

</manifest>